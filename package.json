{
  "name": "plottr-backend",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "build": "tsc -p .",
    "start": "node ./dist/index.js",
    "dev": "ts-node -r tsconfig-paths/register src/index.ts",
    "migrate": "knex --knexfile src/db/knexfile.ts migrate:latest",
    "migrate:rollback": "knex --knexfile src/db/knexfile.ts migrate:rollback",
    "seed": "knex --knexfile src/db/knexfile.ts seed:run",
    "test": "jest",
    "test:migrations": "jest tests/migrations/migrations.test.ts"
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "knex": "^2.0.0",
    "objection": "^3.0.0",
    "pg": "^8.0.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "aws-sdk": "^2.0.0",
    "@mapbox/mapbox-sdk": "^0.13.0"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "ts-node": "^10.0.0",
    "zod": "^3.22.2",
    "tsyringe": "^4.7.0",
    "@types/express": "^4.17.17",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^18.0.0",
    "@types/knex": "^0.16.1",
    "@types/pg": "^8.6.6",
    "eslint": "^8.0.0"
  }
}
