{
  "name": "plottr-backend",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "build": "tsc -p .",
    "start": "node ./dist/index.js",
    "dev": "ts-node -r tsconfig-paths/register src/index.ts",
    "migrate": "knex --knexfile src/db/knexfile.ts migrate:latest",
    "migrate:rollback": "knex --knexfile src/db/knexfile.ts migrate:rollback",
    "seed": "knex --knexfile src/db/knexfile.ts seed:run",
    "db:reset": "knex --knexfile src/db/knexfile.ts migrate:rollback --all && knex --knexfile src/db/knexfile.ts migrate:latest && knex --knexfile src/db/knexfile.ts seed:run",
    "db:seed": "knex --knexfile src/db/knexfile.ts seed:run",
    "db:migrate": "knex --knexfile src/db/knexfile.ts migrate:latest",
    "test": "jest --runInBand --ci --verbose",
    "test:detect": "jest --runInBand --ci --detectOpenHandles --testTimeout=20000",
    "test:unit": "jest --runInBand --testPathIgnorePatterns=\"tests/integration|tests/migrations\" --passWithNoTests",
    "test:migrations": "cross-env NODE_ENV=test jest tests/migrations/migrations.test.ts --runInBand -i",
    "test:integration": "cross-env NODE_ENV=test jest tests/integration --runInBand -i",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "preflight": "npm run check:types && jest --listTests",
    "gen:types": "openapi-typescript openapi/plottr.yaml --output src/types/openapi.ts",
    "check:types": "tsc -p tsconfig.build.json --noEmit"
  },
  "lint-staged": {
    "src/**/*.{ts,js}": [
      "npm run check:types"
    ],
    "tests/**/*.{ts,js}": [
      "npm test -- --silent"
    ]
  },
  "prepare": "husky install",
  "dependencies": {
    "@clerk/backend": "^2.18.3",
    "@mapbox/mapbox-sdk": "^0.13.0",
    "@tmcw/togeojson": "^7.1.2",
    "@turf/area": "^7.2.0",
    "@turf/bbox": "^7.2.0",
    "@turf/boolean-contains": "^7.2.0",
    "@turf/boolean-intersects": "^7.2.0",
    "@turf/centroid": "^7.2.0",
    "@turf/helpers": "^7.2.0",
    "@turf/length": "^7.2.0",
    "@turf/simplify": "^7.2.0",
    "@types/multer": "^2.0.0",
    "@types/xmldom": "^0.1.34",
    "aws-sdk": "^2.0.0",
    "axios": "^1.12.2",
    "dotenv": "^10.0.0",
    "express": "^4.18.2",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.0",
    "knex": "^2.0.0",
    "multer": "^2.0.2",
    "objection": "^3.0.0",
    "openai": "^4.10.0",
    "pg": "^8.0.0",
    "swagger-ui-express": "^5.0.1",
    "tsconfig-paths": "^4.2.0",
    "xmldom": "^0.6.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/knex": "^0.16.1",
    "@types/mapbox__mapbox-sdk": "^0.16.3",
    "@types/node": "^18.0.0",
    "@types/node-fetch": "^2.6.2",
    "@types/pg": "^8.6.6",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.8",
    "cross-env": "^10.1.0",
    "eslint": "^8.0.0",
    "husky": "^8.0.0",
    "jest": "^29.0.0",
    "lint-staged": "^14.0.0",
    "node-fetch": "^2.6.7",
    "openapi-typescript": "^7.10.1",
    "playwright": "^1.56.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.0.0",
    "tsyringe": "^4.7.0",
    "typescript": "^5.0.0",
    "zod": "^3.22.2"
  },
  "optionalDependencies": {
    "sharp": "^0.34.4"
  }
}
